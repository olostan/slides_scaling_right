#lets_bg { 
  ::rotateX(90) up(600) scale(1.2);
  img { width: 100vw; height: 100vh;}
}
#lets { font-size: 6vw; text-shadow: 0 0 12px #000; text-align:center; color: black; width: 90vw; text-align: center;
  ::up(1300) rotateX(90);
}

.s11 {
  #imp_q3: rotateX(-90) down(1200) hide;
  #imp_q4: rotateX(-90) down(800) hide delay(0.2);
  #imp_q5: rotateX(-90) down(800) hide delay(0.4);
  #imp_bg: rotateX(-90) down(1300) hide delay(0.6);

  #lets_bg: show delay(0.6) rotateX(-90) down(680);
  #lets: rotateX(-90) down(1000) show delay(0.8);
}
#lets_how {
  font-size: 4vw; text-shadow: 0 0 12px #000; text-align:center; color: black; width: 90vw; text-align: center;
  ::down(550);
}
.s12 {
  #lets: up(30) delay(0);
  #lets_how: up(280) show;
  #lets_bg: up(100) delay(0);
}
#pockemon {
  ::scale(1.5);
  img { width: 80vw; height: 80vh;}
}
.s13 {  
  #lets_bg: opacity(0.8) scale(0.8) delay(0) hide;
  #lets: hide scale(0.5) delay(0);
  #lets_how: hide scale(0.5) delay(0);
  #pockemon:show scale(1) delay(0.2);
}
#small_bg { 
  ::rotateX(-40) up(500) scale(1.2);
  img { width: 100vw; height: 100vh;}
}
#small { font-size: 6vw; text-shadow: 0 0 20px black; text-align:center; color: white; width: 90vw; text-align: center;
  ::up(500) rotateX(-40);
}


.s14 {
  #pockemon:down(500) hide rotateX(40);
  #small:down(500) show rotateX(40);
  #small_bg:down(500) show rotateX(40) delay(0.4);
}

#save_bg { 
  ::rotateX(-40) up(500) scale(1.2);
  img { width: 100vw; height: 100vh;}
}
#save { font-size: 6vw; text-shadow: 0 0 10px black; text-align:center; color: white; width: 90vw; text-align: center;
  background-color: rgba(0,0,0,0.4);
  ::up(500) rotateX(-40);
}
.s15 {
  #small:down(500) hide rotateX(40);
  #small_bg:down(500) hide rotateX(40) delay(0);
  #save:down(500) show rotateX(40);
  #save_bg:down(500) show rotateX(40) delay(0.2);
}

#dec_bg { 
  ::scale(1.5);
  img { width: 100vw; height: 100vh;}
}
#dec { font-size: 6vw; text-shadow: 0 0 10px black; text-align:center; color: white; width: 90vw; text-align: center;
  background-color: rgba(0,0,0,0.4);
  ::scale(1.5);
  b { color: yellow; }
}
.s16 {
  #save:scale(1.5) hide;
  #save_bg:scale(1.5) hide delay(0.2);
  #dec_bg: scale(1.2) show;
  #dec: scale(1) show delay(0.4);
}
#dec_2 { font-size: 6vw; text-shadow: 0 0 10px black; text-align:center; color: white; width: 90vw; text-align: center;
  background-color: rgba(0,0,0,0.4);
  ::down(600);
  b { color: greenyellow; }
}
.s17 {
  #dec_bg: up(50);
