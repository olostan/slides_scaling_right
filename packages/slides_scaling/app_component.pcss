/* dacsslide.dunits = vmin */
my-app presentation symbol div {
  font-size: 4vw;
  color: white;  
}
my-app presentation symbol[sample] {
  > pre {
    font-size: 2vw;
  }
}
#title1 {
  font-size: 6vw;
  font-stretch: condensed;
  border-bottom: solid 1px black;
  padding-bottom: 20px;
  color: yellow;
}
#title2 {
  font-size: 3vw;
  width: 90vw;
  text-align: center;
  color: #666;
}
body {
  background-color: black;
}
#logo img {
  width: 50vw;
}
#val_photo {
  img {
    width: 200px;
    position: relative;
    top: -30px; left: -20px;

  }
  width: 180px;height: 180px;
  border-radius: 100px;
  overflow: hidden;
}
.s1 {
  #logo: show up(200);
  #title1: show down(20);
  #title2: show down(90);
  #val_photo: show down(220);
  #val: show down(330) scale(0.4);
  #val_url: show down(350) scale(0.4);

}
#def {
  width: 50vw;
  font-size: 2vw;
  text-align: justify;
  ::down(400);
}
.s2 {
  #logo: up(50) opacity(0.9) delay(0);
  #title1{ ::up(80) scale(0.8); color: white; }
  #title2 { 
    color: yellow;
    ::up(80) scale(1.2) delay(0.2);
  }
  #def: show up(200) delay(0.4);
  #val_photo: down(200) delay(0.2) hide;
  #val_url: down(200) delay(0.3) hide;
  #val: down(200) delay(0.4)  hide;
}
#agenda{ ::right(900) down(20); color: yellow;}
#agenda_items{ ::right(900) down(180); 
  li { color: white; font-size: 5vh;}
}
.s3 {
  #title2: left(900) hide;
  #def: left(900) delay(0.4) hide;
  #agenda: left(900) show;
  #agenda_items: left(900) show delay(0.4);
}
#why_bg { 
  ::scale(1.6);
  img { width: 90vw; height: 100vh;}
}
#why { font-size: 10vw; text-shadow: 0 0 12px #000;}
.s4 {
  #logo: up(300) hide;
  #title1: up(200) hide;
  #title2: up(220) hide;
  #agenda: down(220) hide delay(0.2);
  #agenda_items: down(220) hide delay(0);
  #why_bg: show scale(1) delay(0.2);
  #why: show up(180) delay(0.4);
  #why_q: show down(300) delay(0.6);
}
#imp_bg { 
  ::right(400) rotateY(-90) scale(1.2);
  img { width: 100vw; height: 100vh;}
}
#imp { font-size: 6vw; text-shadow: 0 0 12px #000; text-align:center;
  background-color: rgba(100,100,100,0.4);
  ::right(400) rotateY(-90) down(200);
}

.s5 {
  #why: rotateY(90) left(500) hide delay(0.3);
  #why_q: rotateY(90) left(600) hide delay(0.1);
  #why_bg: rotateY(90) left(300) hide delay(0);

  #imp: rotateY(90) left(400) show delay(0.1);
  #imp_bg: rotateY(90) left(400) show;
}
#imp_q1,#imp_q2,#imp_q3,#imp_q4,#imp_q5 {
  background-color: rgba(255,255,255,0.9); color: black; width: 75vw; padding: 20px; font-size:6vh; border: solid 2px red;
}
#imp_q1: down(600);
#imp_q2: down(600);
#imp_q3: down(600);
#imp_q4: down(600);
#imp_q5: down(600);
.s6 {
  #imp_bg: opacity(0.4) scale(1);
  #imp: up(400) opacity(0.4);
  #imp_q1: up(400) show;
}
.s7 {
  #imp: up(400) hide;
  #imp_q1: up(300) scale(0.8) opacity(0.8);
  #imp_q2: up(400) show;
}
.s8 {
  #imp_q1: up(300) scale(0.6) opacity(0.4);
  #imp_q2: up(300) scale(0.8) opacity(0.8);
  #imp_q3: up(400) show;
}
.s9 {
  #imp_q1: up(300) scale(0.4) hide;
  #imp_q2: up(300) scale(0.6) opacity(0.4);
  #imp_q3: up(350) scale(0.8) opacity(0.8);
  #imp_q4: up(430) show;
}
.s10 {
  #imp_q2: up(300) scale(0.4) hide;
